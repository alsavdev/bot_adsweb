{
  "name": "bot-traffic",
  "productName": "bot-traffic",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "dev": "npx tailwindcss -i ./src/index.css -o ./src/assets/output.css --watch",
    "lint": "echo \"No linting configured\"",
    "build": "npx electron-builder build"
  },
  "keywords": [],
  "author": "Alsav System",
  "license": "MIT",
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "fs": "^0.0.1-security",
    "path": "^0.12.7",
    "puppeteer": "^21.6.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-recaptcha": "^3.6.8",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "sweetalert2": "^11.10.1",
    "user-agents": "^1.1.49"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.2.0",
    "electron": "28.0.0"
  },
  "build": {
    "appId": "Alsav Traffic",
    "productName": "Alsav Traffic",
    "compression": "maximum",
    "copyright": "Copyrights 2023 Alsav System",
    "publish": {
      "provider": "github",
      "owner": "alsavdev",
      "repo": "bot_adsweb",
      "private": true
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis",
      "icon": "./src/assets/traffic-3.ico"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true
    },
    "extraFiles": [
      "src/bot/extension/spoof",
      "src/bot/extension/captcha",
      "src/bot/extension/cghost",
      "src/bot/extension/surfshark",
      "src/bot/extension/zenmate"
    ],
    "files": [
      "!src/bot/data/ck.json",
      "!src/bot/data/id.txt",
      "!src/bot/data/idghost.txt",
      "!src/bot/data/idsurf.txt",
      "!src/bot/data/idzen.txt"
    ]
  }
}
