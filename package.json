{
  "name": "bot-traffic",
  "productName": "bot-traffic",
  "version": "1.0.2",
  "description": "My Electron application description",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "dev": "electron .",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "build": "npx electron-builder build",
    "deploy": "electron-builder -p always"
  },
  "keywords": [],
  "author": "Alsav System",
  "license": "MIT",
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^6.1.7",
    "fs": "^0.0.1-security",
    "path": "^0.12.7",
    "puppeteer": "^21.11.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-recaptcha": "^3.6.8",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "sweetalert2": "^11.10.1",
    "user-agents": "^1.1.49"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.2.0",
    "electron": "28.0.0",
    "electron-builder": "^24.9.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alsavdev/bot_adsweb"
  },
  "build": {
    "appId": "Alsav Traffic",
    "productName": "Alsav Traffic",
    "compression": "maximum",
    "copyright": "Copyrights 2024 Alsav System",
    "publish": {
      "provider": "github",
      "owner": "alsavdev",
      "repo": "bot_adsweb",
      "private": true
    },
    "win": {
      "target": "nsis",
      "icon": "/src/assets/traffic-3.ico"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true
    },
    "extraFiles": [
      "src/bot/extension/spoof",
      "src/bot/extension/captcha",
      "src/bot/extension/cghost",
      "src/bot/extension/surfshark",
      "src/bot/extension/zenmate",
      "src/bot/data/"
    ],
    "files": [
      "!dev/"
    ]
  }
}
